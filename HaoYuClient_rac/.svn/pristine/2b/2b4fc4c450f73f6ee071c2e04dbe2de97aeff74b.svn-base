import UIKit

class LWDefaultLable: LWBaseLable {
    
    override init(frame: CGRect) {
        super.init(frame: frame)
        
    }
    required init?(coder aDecoder: NSCoder) {
        fatalError("init(coder:) has not been implemented")
    }
    
    class func defaultLable(text:String,textColor:UIColor,textFont:UIFont) -> LWDefaultLable {
        let la:LWDefaultLable = LWDefaultLable()
        la.text = text
        la.textColor = textColor
        la.font = textFont
        return la
    }
}

