import UIKit

class LWHud: NSObject {
    
    static func show(){
        SVProgressHUD.show()
    }
    static func show(withStatus:String){
        SVProgressHUD.show(withStatus: withStatus)
    }
    static func dismiss(){
        SVProgressHUD.dismiss()
    }
    
}
