@implementation HYBaseView

- (instancetype)initWithFrame:(CGRect)frame
{
    self = [super initWithFrame:frame];
    if (self) {
        self.backgroundColor = HYCOLOR.color_c0;
        MARGIN = ADJUST_PERCENT_FLOAT(10);
    }
    return self;
}
